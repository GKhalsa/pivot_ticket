<div class="row">
  <div class="col s12 m3">
    <div class="collection">
       <%= link_to "Venues", '#', class: "collection-item active" %>
       <% @venues.each do |venue| %>
         <% if venue.active? %>
           <%= link_to venue.name, venue_path(venue.slug), class: "collection-item" %>
         <% end %>
       <% end %>
    </div>
  </div>
