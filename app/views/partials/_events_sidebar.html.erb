<div class="row">
  <div class="col s12 m3">
    <div class="collection">
       <%= display_all_category_link(active_category) %>
       <% @categories.each do |category| %>
         <%= display_category_link(category, active_category) %>
       <% end %>
    </div>
    <br>
    <div class="collection">
       <% unless venues.nil? %>
         <%= link_to "Venues", '#', class: "collection-item active" %>
         <% venues.each do |venue| %>
           <% if venue.active? %>
           <%= link_to venue.name, venue_path(venue.slug), class: "collection-item" %>
           <% end %>
         <% end %>
       <% end %>
     </div>
    </div>
  <!-- </div> -->
