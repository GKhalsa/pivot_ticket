<h3><%= @venue.name %>'s moderators</h3>
<div class="center">
  <%= link_to "Create a New Moderator",
              new_admin_venue_moderator_path(id: @venue.id),
              class: "waves-effect waves-light btn" %>
</div>
<table class="highlight">
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
    <% @moderators.each do |moderator|%>
    <tr>
      <td><%= moderator.name %></td>
      <td><%= moderator.email %></td>
      <td><%= link_to "Remove", admin_venue_moderator_path(moderator), class: "waves-effect waves-light btn" %></td>
      <!-- <td><%#= button_to_change_ticket_state(ticket) %></td> -->
    </tr>
  <% end %>
</table>
