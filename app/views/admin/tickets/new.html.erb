<h5>Create a new Ticket</h5>
<%= form_for([:admin, @ticket], multipart: true) do |f| %>
  <div class="input-field">
    <%= f.label :seat_location %>
    <%= f.text_field :seat_location %>
  </div>
  <div class="input-field">
     <%= f.collection_select :category_id, Category.all, :id, :name, { prompt: "Choose a category" }, class: "browser-default" %>
  </div>
  <%= link_to "Or create a new one", new_admin_category_path, class: "waves-effect waves-light btn" %>
  <div class="input-field">
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>
  <!-- <div class="input-field">
    <%#= f.label :description %>
    <%#= f.text_area :description, class: "materialize-textarea" %>
  </div> -->
    <%= f.label :avatar %>
    <%= f.file_field :avatar %>
    <%= f.label :file %>
    <%= f.file_field :file %>
  <div class="input-field">

  </div>
  <div class="center">
    <%= f.submit "Create Ticket", class: "waves-effect waves-light btn" %>
  </div>
<% end %>
