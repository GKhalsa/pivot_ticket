<h5>Edit Ticket</h5>
<%= form_for([:admin, @ticket], multipart: true) do |f| %>
  <div class="input-field">
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>
  <div class="input-field">
     <%= f.collection_select :category_id, Category.all, :id, :name, { prompt: "Choose a category" }, class: "browser-default" %>
  </div>
  <div class="input-field">
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>

  <div class="center">
    <%= f.submit "Edit Ticket", class: "waves-effect waves-light btn" %>
  </div>
<% end %>
